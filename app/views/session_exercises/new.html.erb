<h1>New Session Exercise</h1>

<%= form_with(model: @session_exercise, local: true) do |form| %>
  <div>
    <%= form.label :session_id %>
    <%= form.number_field :session_id %>
  </div>

  <div>
    <%= form.label :exercise_id %>
    <%= form.number_field :exercise_id %>
  </div>

  <div>
    <%= form.label :weight %>
    <%= form.number_field :weight %>
  </div>

  <div>
    <%= form.label :sets %>
    <%= form.number_field :sets %>
  </div>

  <div>
    <%= form.label :reps %>
    <%= form.number_field :reps %>
  </div>

  <div>
    <%= form.submit "Save" %>
  </div>
<% end %>

<%= link_to 'Back', session_exercises_path %>